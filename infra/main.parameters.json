{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "principalId": {
        "value": "${AZURE_PRINCIPAL_ID}"
      },
      "appExists": {
        "value": "${SERVICE_APP_RESOURCE_EXISTS=false}"
      },
      "azureAdTenantId": {
        "value": "${AZURE_AD_TENANT_ID=}"
      },
      "azureAdClientId": {
        "value": "${AZURE_AD_CLIENT_ID=}"
      },
      "azureSearchEndpoint": {
        "value": "${AZURE_SEARCH_ENDPOINT=}"
      },
      "azureSearchIndex": {
        "value": "${AZURE_SEARCH_INDEX=}"
      },
      "azureSearchApiKey": {
        "value": "${AZURE_SEARCH_API_KEY=}"
      },
      "azureSearchSemanticConfig": {
        "value": "${AZURE_SEARCH_SEMANTIC_CONFIG=}"
      },
      "azureSearchTopN": {
        "value": "${AZURE_SEARCH_TOP_N=5}"
      },
      "azureSearchStrictness": {
        "value": "${AZURE_SEARCH_STRICTNESS=3}"
      },
      "azureStorageConnectionString": {
        "value": "${AZURE_STORAGE_CONNECTION_STRING=}"
      },
      "azureOpenAIEndpoint": {
        "value": "${AZURE_OPENAI_ENDPOINT=}"
      },
      "azureOpenAIKey": {
        "value": "${AZURE_OPENAI_KEY=}"
      },
      "azureOpenAIEmbeddingDeployment": {
        "value": "${AZURE_OPENAI_EMBEDDING_DEPLOYMENT=}"
      },
      "allowedOrigins": {
        "value": "${ALLOWED_ORIGINS=*}"
      },
      "rateLimitUploadsCount": {
        "value": "${RATE_LIMIT_UPLOADS_COUNT=10}"
      },
      "rateLimitUploadsWindow": {
        "value": "${RATE_LIMIT_UPLOADS_WINDOW=3600}"
      },
      "rateLimitApiCount": {
        "value": "${RATE_LIMIT_API_COUNT=100}"
      },
      "rateLimitApiWindow": {
        "value": "${RATE_LIMIT_API_WINDOW=3600}"
      },
      "rateLimitAdminCount": {
        "value": "${RATE_LIMIT_ADMIN_COUNT=50}"
      },
      "rateLimitAdminWindow": {
        "value": "${RATE_LIMIT_ADMIN_WINDOW=3600}"
      }
    }
  }
